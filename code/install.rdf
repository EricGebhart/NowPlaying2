<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description rdf:about="urn:mozilla:install-manifest">

    <em:id>Songbird-NowPlaying@lindesay.co.uk</em:id>

    <em:localized>
      <Description>
        <em:locale>da-DK</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Tilføjer en afspilningsliste med mulighed for, at trække og slippe sange ind i køen.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>de-DE</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Fügt eine aktuelle Wiedergabeliste hinzu, der man durch einfaches Hinüberziehen Lieder der Liste anhängen kann</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>es-ES</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Añade a la lista de reproducción actual solo con arrastrar y soltar las canciones sobre la cola.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>fr-FR</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Ajoute une liste de lecture en cours (panneau latéral droit) avec la possibilité de glisser-déposer des titres dans la file d'attente.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>ja-JP</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>トラックをキューにドラッグ＆ドロップできる再生中リストを追加します。</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>pl-PL</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Dodaje listę odtwarzanych utworów umożliwiającą dodawanie piosenek do kolejki.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>sl-SL</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>Doda trenutni spored skladb z možnostjo, da v vrsto povlečete in spustite pesmi.</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>zh-TW</em:locale>
        <em:name>Now Playing List 2</em:name>
        <em:description>加入「正在播放清單」的功能，讓你可以拖放歌曲到佇列中依序播放。</em:description>
      </Description>
    </em:localized>
    <em:name>Now Playing List 2</em:name>
    <em:description>Adds a now playing list with ability to drag and drop songs into the queue</em:description>

    <em:version>2.1.2</em:version>
    <em:creator>Forbes Lindesay</em:creator>

    	<em:contributor>James Moschou(Creator of the old version)</em:contributor>
	<em:contributor>Joergen (da-DK Translation)</em:contributor>
	<em:contributor>Lukas_Skywalker (de-DE Translation)</em:contributor>
	<em:contributor>Nukeador (es-ES Translation)</em:contributor>
	<em:contributor>Goofy (fr-FR/es-ES Translation)</em:contributor>
	<em:contributor>mar (ja-JP Translation)</em:contributor>
	<em:contributor>Wacław Jacek (pl-PL Translation)</em:contributor>
	<em:contributor>miles (sl-SL Translation)</em:contributor>
	<em:contributor>josesun (zh-TW Translation)</em:contributor>
	<em:contributor>eagebhart (fade to next)</em:contributor>

    <em:optionsURL>chrome://nowplaying/content/xul/prefsDummyWindow.xul</em:optionsURL>
    <em:iconURL>chrome://nowplaying/content/icons/main.png</em:iconURL>
    <em:homepageURL></em:homepageURL>
    <!-- Nightingale -->
		<em:targetApplication>
			<Description>
				<em:id>nightingale@getnightingale.com</em:id>
				<em:minVersion>1.11.0a</em:minVersion>
				<em:maxVersion>2.2.0a</em:maxVersion>
			</Description>
		</em:targetApplication>
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>1.5.0</em:minVersion>
        <em:maxVersion>1.8.9</em:maxVersion>
      </Description>
    </em:targetApplication>

    <songbird:displayPane>
      <Description>
        <songbird:contentUrl>chrome://nowplaying/content/xul/nowplaying.xul</songbird:contentUrl>
        <songbird:contentTitle>Now Playing</songbird:contentTitle>
        <songbird:contentIcon>chrome://nowplaying/content/icons/favicon.png</songbird:contentIcon>
        <songbird:defaultWidth>260</songbird:defaultWidth>
        <songbird:defaultHeight>194</songbird:defaultHeight>
        <songbird:suggestedContentGroups>sidebar;contentpane</songbird:suggestedContentGroups>
        <songbird:showOnInstall>true</songbird:showOnInstall>
      </Description>
    </songbird:displayPane>

  </Description>
</RDF>
